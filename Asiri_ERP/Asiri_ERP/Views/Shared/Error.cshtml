@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-title">
                <h1 class="text-danger">Error.</h1>
            </div>
            <hr />
            <h2 class="text-danger">Se produjo un error al procesar su solicitud.</h2>
            <p>
                @ViewBag.Error
            </p>
        </div>
    </div>
</div>

            
